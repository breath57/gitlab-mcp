# GitLab MCP Server 环境变量配置模板
# 复制这个文件为 .env 并填入您的实际值

# GitLab Personal Access Token (必需)
GITLAB_PERSONAL_ACCESS_TOKEN=AkmtDpdVvLmgdmTyFBKQ

# GitLab API URL (必需)
GITLAB_API_URL=https://git.n.xiaomi.com/api/v4

# 默认项目 ID (可选)
GITLAB_PROJECT_ID=

# 只读模式 (可选，默认为 false)
GITLAB_READ_ONLY_MODE=true

# Wiki 功能 (可选，默认为 false)
USE_GITLAB_WIKI=false

# 里程碑功能 (可选，默认为 false)
USE_MILESTONE=false

# 流水线功能 (可选，默认为 false)
USE_PIPELINE=false

# Cookie 认证文件路径 (可选)
GITLAB_AUTH_COOKIE_PATH=

# SSE 模式 (可选，默认为 false)
SSE=false

# Streamable HTTP 模式 (可选，默认为 false)
STREAMABLE_HTTP=false

# 旧版 GitLab 兼容模式 (可选，默认为 false)
GITLAB_IS_OLD=false 